 implementation bg
  submodels
   Controller 272 192
    description '<Description><Version>4.0</Version><IsMainModel>1</IsMainModel><KeepParameterValues>False</KeepParameterValues>
   <LibraryPath>Signal\Control\PID Control\Discrete\Controller-P.emx</LibraryPath>
  <TimeStamp>2007-9-27 14:51:13</TimeStamp>
</Description>';
    type 'Controller-P'
     ports
      signal in SP;
      signal in MV;
      signal out output;
    end;
    icon bg bottom
     figures
      rectangle 240 160 304 224 fill 15132390;
      text 'P' 288.5 191 color 1336868 18 bold;
      text 'SP' 252.8 174.9 color 1336868 14;
      text 'MV' 254.9 205.9 color 1336868 14;
     terminals
      SP 240 176 fixed;
      MV 240 208 fixed;
      output 304 192 fixed;
    end;
    implementation eq
// Created by C. Kleijn, 22 Sep 1999
parameters
	real K = 1.0 {};			// Proportional gain
	real a = -0.3 {};
	real b = 1.0 {};
variables
	real dx;
equations
	dx = a*MV + b*SP;
	output = K * dx;
                             implementation_end;
   LinearSystem 424 192
    description '<Description><Version>4.0</Version><IsMainModel>1</IsMainModel><KeepParameterValues>False</KeepParameterValues>
   <LibraryPath>Signal\Discrete\LinearSystem.emx</LibraryPath>
  <TimeStamp>2007-9-27 15:20:24</TimeStamp>
  <AllowLibraryUpdate>False</AllowLibraryUpdate>
</Description>';
    type LinearSystem
     ports
      signal in u;
      signal out y;
    end;
    icon bg bottom
     figures
      rectangle 409 177 439 207 fill 15132390;
      text 'A B' 424 185 color 16711680 bold;
      text 'C D' 424 198 color 16711680 bold;
    end;
    implementation eq
// 20-sim Linear System Editor
// abcd h 0.1
parameters
	real hidden A [1, 1] = [1.0];
	real hidden B [1, 1] = [1.0];
	real hidden C [1, 1] = [1.0];
	real hidden x0 [1] = [0.0];
variables
	real x [1];
equations
	next (x, x0) = A * x + B * u;
	y = C * x;
implementation_end;
   Step 128 176
    description '<Description><Version>4.0</Version><IsMainModel>1</IsMainModel><KeepParameterValues>False</KeepParameterValues>
   <LibraryPath>Signal\Sources\SignalGenerator-Step.emx</LibraryPath>
  <TimeStamp>2007-9-27 16:2:44</TimeStamp>
</Description>';
    type 'SignalGenerator-Step'
     ports
      signal out output;
    end;
    icon bg bottom
     figures
      group
       rectangle 112 160 144 192 fill 15132390;
       line 121.6 172.8 138.6 172.8 color 16711680 width 2;
       line 114.9 184 141.1 184;
       line 117.1 184.1 121.7 184.1 121.7 172.7 color 16711680 width 2;
       line 117.1 168 117.1 186.2;
      end;
    end;
    implementation eq
parameters
	real amplitude = 1;
	real start_time = 2.0 {s};
variables
	boolean hidden change;
equations
	"calculate at least at the start time"
	change = timeevent (start_time);
	
	"calculate the step signal"
	output = amplitude * step (start_time);     implementation_end;
  end;
  connections
   Controller\output -> LinearSystem\u;
   LinearSystem\y -> Controller\MV 424 256 208 256 208 208;
   Step\output -> Controller\SP;
  end;
 implementation_end;
]]>
